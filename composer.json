        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.21"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DavidPeach\\LaravelHmacValidationRule\\": "src/"
        }
    },
